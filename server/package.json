{
	"name": "flawk-backend",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"node": "14.15.0"
	},
	"scripts": {
		"test": "jest --coverage",
		"testCI": "jest",
		"prettier": "prettier --write ./app",
		"prettierCI": "prettier --check ./",
		"eslint": "eslint --quiet ./",
		"auditCI": "npm audit --audit-level=moderate"
	},
	"jest": {
		"testEnvironment": "node",
		"testPathIgnorePatterns": [
			"/node_modules/"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/coverage/"
		],
		"collectCoverageFrom": [
			"**/*.{js,jsx}",
			"!**/node_modules/**"
		]
	},
	"dependencies": {
		"@awaitjs/express": "^0.6.3",
