	"_____homepage": "./",
	"scripts": {
		"copyCordova": "babel-node copy_cordova.js",
		"copyPublic": "babel-node copy_public.js",
		"start": "npm run copyPublic && rescripts start",
		"build": "CI=false SKIP_PREFLIGHT_CHECK=true GENERATE_SOURCEMAP=true react-scripts --max_old_space_size=4096 build",
		"buildLocal": "npm run auditCI & npm run test & rescripts --max_old_space_size=4096 build",
		"buildCordova": "npm run auditCI & npm run test & rescripts --max_old_space_size=4096 build && npm run copyCordova",
		"test": "set CI=true && react-scripts test --coverage",
		"testCI": "CI=true react-scripts test",
		"eject": "react-scripts eject",
		"prettier": "prettier --write ./src",
		"prettierCI": "prettier --check ./",
		"auditCI": "npm audit --audit-level=moderate"
	},
