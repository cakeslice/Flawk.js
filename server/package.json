		"eslint-plugin-jest": "^24.1.0",
		"eslint-plugin-jsdoc": "^30.6.5",
		"eslint-plugin-no-secrets": "^0.6.8",
		"eslint-plugin-security": "^1.4.0",
		"eslint-plugin-security-node": "^1.0.12",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.2"
	}
