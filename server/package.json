		"@vonage/server-sdk": "^2.10.4",
		"aws-sdk": "^2.767.0",
		"bcryptjs": "^2.4.3",
		"cachegoose": "^8.0.0",
		"compression": "^1.7.4",
		"connect-mongo": "^3.2.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"crypto-extra": "^1.0.1",
		"delay": "^4.4.0",
		"diacritic-regex": "^1.1.0",
		"express": "^4.17.1",
		"express-mongo-sanitize": "^2.0.0",
		"express-openapi-validator": "^4.12.7",
		"express-paginate": "^1.0.2",
		"helmet": "^4.1.1",
		"jest": "^26.5.0",
