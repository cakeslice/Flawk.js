{
	"name": "flawk-frontend",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"node": "14.15.0"
	},
	"lint-staged": {
		"*.{js,jsx,css,less,scss,ts,tsx,md}": [
			"eslint --quiet",
			"prettier --check"
		]
	},
	"scripts": {
		"copyPublic": "babel-node copy_public.js",
		"addPreloadLinks": "babel-node add_preload_links.js",
		"start": "npm run copyPublic && rescripts start",
		"build": "CI=false SKIP_PREFLIGHT_CHECK=true GENERATE_SOURCEMAP=true react-scripts --max_old_space_size=4096 build && npm run addPreloadLinks",
		"buildFast": "npm run copyPublic && rescripts build && npm run addPreloadLinks",
		"buildLocal": "npm run copyPublic && npm run auditFix & npm run test & rescripts build && npm run addPreloadLinks",
		"buildLocalFast": "npm run copyPublic & rescripts build & npm run addPreloadLinks",
		"run_ios": "npm run copyPublic && rescripts build && npx cap sync && npx cap open ios && npm run addPreloadLinks",
		"test": "set CI=true && react-scripts test --coverage",
		"testCI": "CI=true react-scripts test",
		"eject": "react-scripts eject",
		"prettier": "prettier --write ./src",
		"prettierCI": "prettier --check ./",
		"lint": "eslint --quiet ./",
		"auditFix": "npm audit fix",
		"auditCI": "npm audit --audit-level=moderate",
		"precommit": "lint-staged",
		"postinstall": "cd .. & git config core.hooksPath ./git-hooks"
	},
	"dependencies": {
		"@capacitor/android": "^2.4.6",
		"@capacitor/cli": "^2.4.6",
		"@capacitor/core": "^2.4.6",
		"@capacitor/ios": "^2.4.6",
		"@sentry/react": "^6.8.0",
		"@sentry/tracing": "^6.8.0",
		"abortcontroller-polyfill": "^1.7.1",
		"body-scroll-lock": "^3.1.5",
		"browserslist": "^4.16.6",
		"canvas-to-blob": "^1.0.0",
		"countries-list": "^2.6.1",
		"expo-splash-screen": "^0.6.2",
		"formik": "^2.2.0",
		"glamor": "^2.20.40",
		"html-react-parser": "^0.14.0",
		"local-storage-fallback": "^4.1.1",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"node-sass": "^4.14.1",
		"numeral": "^2.0.6",
		"prop-types": "^15.7.2",
		"rc-slider": "^9.5.4",
		"react": "^17.0.1",
		"react-animated-css": "^1.2.1",
		"react-awesome-lightbox": "^1.7.3",
		"react-beforeunload": "^2.4.0",
		"react-collapse": "^5.0.1",
		"react-copy-to-clipboard": "^5.0.2",
		"react-datetime": "^3.0.4",
		"react-dom": "^17.0.1",
		"react-dropzone": "^11.2.4",
		"react-elastic-carousel": "^0.11.4",
		"react-flagkit": "^2.0.4",
		"react-ga": "^3.2.0",
		"react-google-recaptcha": "^2.1.0",
		"react-helmet": "^6.1.0",
		"react-image": "^4.0.3",
		"react-input-mask": "^2.0.4",
		"react-input-range": "^1.3.0",
		"react-json-view": "^1.21.3",
		"react-popper-tooltip": "^3.1.0",
		"react-portal": "^4.2.1",
		"react-quill": "^1.3.5",
		"react-redux": "^7.2.1",
		"react-responsive": "^8.1.0",
		"react-reveal": "^1.2.2",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.1",
		"react-select": "^3.1.0",
		"react-spinners-kit": "^1.9.1",
		"react-to-print": "^2.12.3",
		"react-toastify": "^6.0.9",
		"react-transition-group": "^4.4.1",
		"redux": "^4.0.5",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"scroll-to-element": "^2.0.3",
